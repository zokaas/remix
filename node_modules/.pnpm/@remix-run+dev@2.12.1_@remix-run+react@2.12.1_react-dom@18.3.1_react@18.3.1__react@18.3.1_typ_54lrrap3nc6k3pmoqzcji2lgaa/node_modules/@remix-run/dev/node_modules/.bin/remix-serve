#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/serve/dist/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/serve/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/serve/dist/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/serve/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules/@remix-run/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/@remix-run+serve@2.12.1_typescript@5.6.2/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../serve/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../serve/dist/cli.js" "$@"
fi
