#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules/tailwindcss/lib/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules/tailwindcss/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules/tailwindcss/lib/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules/tailwindcss/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@swc+core@1.7.26_@swc+helpers@0.5.13__@types+node@22.7.5_typescript@5.6.2_/node_modules:/Users/zoranaopr/Documents/frontend/remix-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
